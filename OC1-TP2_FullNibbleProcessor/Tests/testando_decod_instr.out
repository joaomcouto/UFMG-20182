S_0x7ffff4ccd8e0 .scope module, "testbench_decod_instr" "testbench_decod_instr" 2 1;
 .timescale 0 0;
v0x7ffff4cee090_0 .net "addr", 4 0, v0x7ffff4ccdcb0_0;  1 drivers
v0x7ffff4cee170_0 .var "instrucao", 23 0;
