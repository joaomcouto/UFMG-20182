v0x7ffff4ccdcb0_0 .var "addr", 4 0;
v0x7ffff4cedc10_0 .net "instrucao", 23 0, v0x7ffff4cee170_0;  1 drivers
v0x7ffff4cedcf0_0 .var "op_code", 2 0;
v0x7ffff4cedde0_0 .var "x", 7 0;
